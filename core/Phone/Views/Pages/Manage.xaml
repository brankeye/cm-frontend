<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="cm.frontend.core.Phone.Views.Pages.Manage">
  <ContentPage.Content>
    <StackLayout>
      <Label Text="Manage" HorizontalTextAlignment="Center" VerticalTextAlignment="Center"></Label>
      <StackLayout Orientation="Horizontal">
        <Label Text="Join" HorizontalOptions="StartAndExpand"></Label>
        <Label Text="Create" HorizontalOptions="EndAndExpand"></Label>
      </StackLayout>
      <Entry Placeholder="School name"></Entry>
      <Button Text="Ok"></Button>
    </StackLayout>
  </ContentPage.Content>
</ContentPage>